+    else if ( r->AsString().BeginsWith("YVSPT") ) ok = r->IsInRange(pair4Momentum->Pt(),pair4Momentum->Rapidity());
+    else if ( r->Quantity() == "NTRCORRPT" ){
+
+      TList* list = static_cast<TList*>(Event()->FindListObject("NCH"));
