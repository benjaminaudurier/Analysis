+  }
+
+  TIter next(&pathNames);
+  TObjString* pathName;
+
+  while ( ( pathName = static_cast<TObjString*>(next()) ) )
+  {
+    THnSparse* h(0x0);
+    h = new THnSparseT<TArrayF>(hname,htitle,2,nbinsx,xmin,xmax);
+    h->Sumw2();
+
+    HistogramCollection()->Adopt(pathName->String().Data(),h);
